{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "trailing-comma": [false],
        "no-duplicate-variable": true,
        "no-unused-variable": [
            true
        ],
        "no-var-keyword": true,
        "ordered-imports": [
            true,
            {
                "import-sources-order": "case-insensitive",
                "named-imports-order": "case-insensitive"
            }
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": [
            true
        ]
    },
    "rulesDirectory": []
}